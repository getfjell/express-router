{
  "name": "@fjell/express-router",
  "version": "4.4.47-dev.0",
  "license": "Apache-2.0",
  "keywords": [
    "express",
    "router",
    "fjell"
  ],
  "description": "Express Router for Fjell",
  "engines": {
    "node": ">=21"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "build": "npm run clean && node build.js",
    "dev": "nodemon --watch src --ext ts --exec 'npm run build'",
    "lint": "eslint . --ext .ts --fix",
    "clean": "rm -rf dist",
    "test": "npm run lint && vitest run --coverage",
    "precommit": "npm run clean && npm run lint && npm run build && npm run test",
    "prepublishOnly": "npm run clean && npm run build",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "cd docs && npm run build",
    "docs:preview": "cd docs && npm run preview",
    "docs:test": "cd docs && npm run test"
  },
  "dependencies": {
    "@fjell/core": "^4.4.42",
    "@fjell/lib": "^4.4.47",
    "@fjell/logging": "^4.4.47",
    "@fjell/registry": "^4.4.42",
    "deepmerge": "^4.3.1",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@fjell/eslint-config": "^1.1.25",
    "@tsconfig/recommended": "^1.0.10",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "0.25.9",
    "eslint": "^9.33.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getfjell/express-router.git"
  }
}