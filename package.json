{
  "name": "@fjell/express-router",
  "version": "4.4.23",
  "license": "Apache-2.0",
  "keywords": [
    "express",
    "router",
    "fjell"
  ],
  "description": "Express Router for Fjell",
  "engines": {
    "node": ">=21"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "build": "npm run clean && node build.js",
    "dev": "nodemon --watch src --ext ts --exec 'npm run build'",
    "lint": "eslint . --ext .ts --fix",
    "clean": "rm -rf dist",
    "test": "npm run lint && vitest run --coverage",
    "prepublishOnly": "npm run clean && npm run build",
    "docs:dev": "cd docs && npm run dev",
    "docs:build": "cd docs && npm run build",
    "docs:preview": "cd docs && npm run preview",
    "docs:test": "cd docs && npm run test"
  },
  "dependencies": {
    "@fjell/core": "file:../fjell-core",
    "@fjell/docs-template": "file:../fjell-docs-template",
    "@fjell/lib": "file:../fjell-lib",
    "@fjell/logging": "file:../fjell-logging",
    "@fjell/registry": "file:../fjell-registry",
    "deepmerge": "^4.3.1",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@fjell/eslint-config": "file:../fjell-eslint-config",
    "@tsconfig/recommended": "^1.0.10",
    "@types/express": "^5.0.3",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.0.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/getfjell/express-router.git"
  }
}
